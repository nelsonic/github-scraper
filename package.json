    "node": ">= 10"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jshint": "^2.11.0",
    "live-server": "^1.2.1",
    "mkdirp": "^3.0.1",
    "pre-commit": "1.2.2",
